{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Survey Results - Culture Amp",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src open:back lint:watch",
    "open:src": "babel-node client/tools/srcServer.js",
    "open:back": "babel-node server/server.js",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "open:dist": "babel-node client/tools/distServer.js",
    "build": "npm run clean-dist && babel-node client/tools/build.js && npm-run-all --parallel open:dist open:back",
    "lint": "esw webpack.config.* client/src client/tools server --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha client/tools/testServer.js client/**/*.spec.js --reporter spec",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caritosteph/surveyresults-cultureamp.git"
  },
  "author": "Carolyn Ulfe",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "fs-promise": "^1.0.0",
    "jquery": "^3.1.1",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-router": "3.0.0",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "^6.18.0",
    "bootstrap-sass": "^3.3.7",
    "browser-sync": "2.18.5",
    "chai": "^3.5.0",
    "css-loader": "0.26.1",
    "enzyme": "^2.7.1",
    "es6-promise": "^4.0.5",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "jsdom": "^9.10.0",
    "json-loader": "0.5.4",
    "mocha": "^3.2.0",
    "node-sass": "4.0.0",
    "npm-run-all": "3.1.2",
    "react-addons-create-fragment": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-paginate": "^4.1.1",
    "request": "^2.79.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.0",
    "sinon": "^1.17.7",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5"
  }
}
